{
  "name": "@pixobe/stencil-components",
  "version": "0.0.17",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-components/stencil-components.esm.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pixobe/stencil-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^4.35.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@stencil/sass": "^3.2.1",
    "@storybook/addon-docs": "^9.0.9",
    "@storybook/addon-links": "^9.0.9",
    "@storybook/html-vite": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "jest": "^30.0.0",
    "jest-cli": "^30.0.0",
    "puppeteer": "^24.10.1",
    "storybook": "^9.0.9"
  },
  "license": "MIT",
  "overrides": {
    "storybook": "$storybook"
  }
}
