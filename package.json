{
  "name": "@pixobe/stencil-components",
  "version": "0.0.27",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-components/stencil-components.esm.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pixobe/stencil-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6007 --no-open",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@pixobe/ui-utils": "^1.0.15",
    "@stencil/core": "^4.36.2",
    "@stencil/sass": "^3.2.2",
    "@stencil/storybook-plugin": "^0.4.2",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.18",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "jest": "^30.0.5",
    "jest-cli": "^30.0.5",
    "puppeteer": "^24.15.0",
    "storybook": "^9.0.18"
  },
  "license": "MIT",
  "overrides": {
    "storybook": "$storybook"
  },
  "dependencies": {
    "@melloware/coloris": "^0.25.0",
    "@storybook/html-vite": "^9.0.18",
    "@thednp/color-picker": "^2.0.4",
    "@yaireo/color-picker": "^0.15.1",
    "alwan": "^2.1.1"
  }
}
