{
  "name": "@pixobe/stencil-components",
  "version": "0.0.39-alpha5",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./components/*": "./dist/components/*.js"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6007 --no-open",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@pixobe/ui-utils": "^1.0.15",
    "@stencil/core": "^4.39.0",
    "@stencil/sass": "^3.2.3",
    "@stencil/storybook-plugin": "^0.5.6",
    "@storybook/addon-docs": "^10.1.7",
    "@storybook/addon-links": "^10.1.7",
    "@storybook/html-vite": "^10.1.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "eslint-plugin-storybook": "10.1.7",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "puppeteer": "^24.32.1",
    "storybook": "^10.1.7"
  },
  "license": "MIT",
  "overrides": {
    "storybook": "$storybook"
  }
}
